<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <ConstraintSet android:id="@+id/start" >
        <Constraint
            motion:layout_editor_absoluteY="-21dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="-16dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView" />
        <Constraint
            android:layout_height="44dp"
            android:layout_width="182dp"
            android:id="@+id/app_name"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.298"
            motion:layout_constraintHorizontal_bias="0.6" />
        <Constraint
            android:layout_height="67dp"
            android:layout_width="78dp"
            android:id="@+id/note_book"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.214"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.302" />
        <Constraint
            android:layout_height="44dp"
            android:layout_width="68dp"
            android:id="@+id/pen"
            android:rotation="7"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.269"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.263" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/user_name"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.583"
            motion:layout_constraintHorizontal_bias="0.497"
            android:alpha="0.0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/welcome_submit"
            motion:layout_constraintTop_toBottomOf="@+id/user_name"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.39"
            motion:layout_constraintHorizontal_bias="0.498"
            android:alpha="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/display_logo" >
        <Constraint
            motion:layout_constraintGuide_begin="140dp"
            android:id="@+id/guideline2" />
        <Constraint
            android:layout_height="35dp"
            android:layout_width="163dp"
            android:id="@+id/app_name"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.62"
            motion:layout_constraintVertical_bias="0.425" />
        <Constraint
            android:id="@+id/note_book"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="80dp"
            android:layout_height="83dp"
            motion:layout_constraintHorizontal_bias="0.196"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.425" />
        <Constraint
            android:id="@+id/pen"
            android:layout_width="62dp"
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.378"
            motion:layout_constraintHorizontal_bias="0.237" />
        <Constraint
            android:id="@+id/imageView3"
            android:layout_width="62dp"
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.378"
            motion:layout_constraintHorizontal_bias="0.237" />
        <Constraint
            android:id="@+id/imageView2"
            android:layout_width="80dp"
            android:layout_height="83dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.196"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.425" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/user_name"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.648"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            motion:layout_editor_absoluteY="566dp"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/welcome_submit"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/display_logo"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="cubic(0.2,0.21,0.59,0.59)"
        motion:pathMotionArc="none"
        motion:duration="1200">
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/app_name"
                motion:framePosition="0"
                android:alpha="0.1" />
            <KeyAttribute
                motion:motionTarget="@+id/app_name"
                motion:framePosition="100"
                android:alpha="1.0" />
            <KeyAttribute
                motion:motionTarget="@+id/note_book"
                motion:framePosition="0"
                android:alpha="0.1" />
            <KeyAttribute
                motion:motionTarget="@+id/note_book"
                motion:framePosition="100"
                android:alpha="1.0" />
            <KeyAttribute
                motion:motionTarget="@+id/pen"
                motion:framePosition="0"
                android:alpha="0.1" />
            <KeyAttribute
                motion:motionTarget="@+id/pen"
                motion:framePosition="100"
                android:alpha="1.0" />
            <KeyAttribute
                motion:motionTarget="@+id/user_name"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/welcome_submit"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/user_name"
                motion:framePosition="100"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/welcome_submit"
                motion:framePosition="100"
                android:alpha="0.0" />
        </KeyFrameSet>
    </Transition>
    <ConstraintSet
        android:id="@+id/first_login"
        motion:deriveConstraintsFrom="@+id/display_logo" >
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/user_name"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.623" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/display_logo"
        motion:constraintSetEnd="@+id/first_login"
        motion:autoTransition="animateToEnd" >
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/user_name"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/welcome_submit"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/user_name"
                motion:framePosition="100"
                android:alpha="1.0" />
            <KeyAttribute
                motion:motionTarget="@+id/welcome_submit"
                motion:framePosition="100"
                android:alpha="1.0" />
        </KeyFrameSet>
    </Transition>
</MotionScene>